<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_focused="true" android:state_checked="true" android:drawable="@drawable/checkbox_checked_bg" />
    <item android:state_focused="true" android:state_checked="false" android:drawable="@drawable/checkbox_unchecked_bg" />
    <item android:state_enabled="false" android:drawable="@drawable/checkbox_disable_bg" />
    <item android:state_checked="false" android:drawable="@drawable/checkbox_unchecked_bg" />
    <item android:state_checked="true" android:drawable="@drawable/checkbox_checked_bg" />
</selector>
